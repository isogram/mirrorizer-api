{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "~2.2",
        "league/flysystem": "^1.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/mail": "^5.4",
        "google/apiclient": "^2.0",
        "kunalvarma05/dropbox-php-sdk": "^0.2.0",
        "predis/predis" : "~1.0",
        "illuminate/redis": "^5.4",
        "kunalvarma05/onedrive-client": "^1.0",
        "stevenmaguire/oauth2-microsoft": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
